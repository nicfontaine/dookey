{
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended"
	],
	"overrides": [
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": [
		"react"
	],
	"rules": {
		"strict": ["error", "global"],
		"func-style": ["error", "expression"],
		"react/react-in-jsx-scope": "off",
		"arrow-parens": ["error", "always"]
	}
}
